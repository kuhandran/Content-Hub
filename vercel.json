{
  "buildCommand": "npm run build",
  "env": {
    "NODE_ENV": "production",
    "ALLOWED_IP": "@allowed_ip",
    "AUTH_USER": "@auth_user",
    "AUTH_PASS": "@auth_pass"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ]
}
