{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "env": {
    "NODE_ENV": "production",
    "GITHUB_TOKEN": "@github_token",
    "AUTH_USER": "@auth_user",
    "AUTH_PASS": "@auth_pass",
    "PORT": "3001"
  },
  "functions": {
    "server.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
